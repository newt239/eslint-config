{
  "name": "newt-eslint-config",
  "engines": {
    "node": ">=18"
  },
  "repository": "newt239/eslint-config",
  "version": "0.1.11",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "author": "newt239",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "@vitest/ui": "^3.2.4",
    "tsdown": "^0.14.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^15.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "typescript-eslint": "^8.42.0"
  }
}
